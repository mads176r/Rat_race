<Window x:Class="RatRaceWPF2.CreateRat"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RatRaceWPF2"
        mc:Ignorable="d"
        Title="CreateRat" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="RatNameInputBox" HorizontalAlignment="Center" Margin="0,165,0,0" TextWrapping="Wrap" Text="Rat name" VerticalAlignment="Top" Width="120"/>
        <Button Content="Submit" HorizontalAlignment="Center" Margin="0,197,0,0" VerticalAlignment="Top" Click="CreateRatButton"/>
    </Grid>
</Window>
